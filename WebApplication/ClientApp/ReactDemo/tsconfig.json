{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "module": "commonjs",
    "moduleResolution": "Node",
    "sourceMap": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "target": "ES5",
    "jsx": "react",
    "outDir": "../../wwwroot/js/reactDemo",

    "lib": [
      "dom",
      "es2015"
    ]

  },
    "include": [
        "src/**/*"
    ]
}
